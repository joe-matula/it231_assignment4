<p id="notice"><%= notice %></p>
<div class="panel panel-default">
  <div class="panel-heading">
    <p>
      <strong>Name:</strong>
      <%= @user.name %>
    </p>
  </div>
  <div class="panel-body">
    <p>
      <strong>Email:</strong>
      <%= @user.email %>
    </p>
  
  <div class="panel-footer">
    <%= link_to 'Edit', edit_user_path(@user) %> |
    <%= link_to 'Back', users_path %>
  </div>
</div>